{
	"name": "@ayka/fiji",
	"version": "2.0.0-rc.6",
	"type": "module",
	"license": "MIT",
	"description": "@ayka/fiji is a configuration management library that provides a flexible and type-safe way to handle application configurations. It supports environment variables, default values, and secret management.",
	"keywords": [
		"config",
		"configuration",
		"management",
		"environment",
		"env",
		"dotenv",
		"secrets",
		"settings",
		"type-safe"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AndreyMork/fiji.git"
	},
	"files": [
		"dist/"
	],
	"types": "./dist/Main.d.ts",
	"exports": {
		".": {
			"types": "./dist/Main.d.ts",
			"import": "./dist/Main.js",
			"require": "./dist/Main.cjs"
		}
	},
	"scripts": {
		"prepack": "just prepack",
		"preversion": "just full-check-strict prepack",
		"test": "just test"
	},
	"dependencies": {
		"@ayka/domistrukt": "2.0.0-rc.3",
		"@glitchdotcom/dotenv": "^1.1.0",
		"immutable": "^4.3.7",
		"znv": "^0.4.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@ayka/biome-config": "^1.0.0",
		"@ayka/tsconfig": "^1.0.0",
		"@biomejs/biome": "^1.9.3",
		"@japa/expect": "^3.0.2",
		"@japa/expect-type": "^2.0.2",
		"@japa/file-system": "^2.3.0",
		"@japa/runner": "^3.1.4",
		"@stryker-mutator/core": "^8.6.0",
		"@stryker-mutator/typescript-checker": "^8.6.0",
		"@swc/core": "^1.7.26",
		"@types/node": "^22.7.4",
		"c8": "^10.1.2",
		"fast-check": "^3.22.0",
		"npm-check-updates": "^17.1.3",
		"prettier": "^3.3.3",
		"tsup": "^8.3.0",
		"tsx": "^4.19.1",
		"typescript": "^5.6.2"
	},
	"imports": {
		"#PackageJSON": [
			"./package.json"
		],
		"#Main": [
			"./src/Main.ts"
		],
		"#Src/*": [
			"./src/*"
		]
	}
}
